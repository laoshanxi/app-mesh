{
  "Description": "myhost",
  "ScheduleIntervalSeconds": 2,
  "RestListenPort": 6060,
  "RestListenAddress": "0.0.0.0",
  "RestEnabled": true,
  "LogLevel": "DEBUG",
  "SSLEnabled": true,
  "SSLCertificateFile": "server.crt",
  "SSLCertificateKeyFile": "server.key",
  "HttpThreadPoolSize": 6,
  "JWTEnabled": true,
  "JWTRedirectUrl": "",
  "Applications": [
    {
      "command": "ping www.baidu.com -w 60",
      "name": "ping",
      "user": "root",
      "cache_lines": 20
    }
  ],
  "JWT": {
    "admin": {
      "key": "Admin123",
      "roles": [ "manage", "view" ],
      "locked": false
    },
    "user": {
      "key": "password",
      "roles": [ "view" ],
      "locked": false
    },
    "test": {
      "key": "test",
      "roles": [ ],
      "locked": true
    }
  },
  "Labels": {
    "os_version": "centos7.6",
    "arch": "x86_64"
  },
  "Roles": {
    "manage": [
      "app-reg",
      "app-reg-shell",
      "app-control",
      "app-delete",
      "run-app-async",
      "run-app-sync",
      "file-download",
      "file-upload",
      "label-view",
      "label-update",
      "label-set",
      "label-delete",
      "log-level",
      "config-set",
      "change-passwd",
      "lock-user",
      "unlock-user"
    ],
    "view": [
      "view-app",
      "view-app-output",
      "view-all-app",
      "view-host-resource",
      "run-app-async-output",
      "label-view",
      "config-view"
    ]
  }
}
