{
  "Description": "myhost",
  "ScheduleIntervalSeconds": 2,
  "RestListenPort": 6060,
  "RestListenAddress": "0.0.0.0",
  "RestEnabled": true,
  "LogLevel": "DEBUG",
  "SSLEnabled": true,
  "SSLCertificateFile": "server.crt",
  "SSLCertificateKeyFile": "server.key",
  "HttpThreadPoolSize": 6,
  "JWTEnabled": true,
  "Applications": [
    {
      "command": "/bin/sleep 60",
      "name": "sleep",
      "user": "root",
      "working_dir": "/tmp"
    }
  ],
  "jwt": {
    "admin": {
      "name": "admin",
      "key": "app-mgr-admin-secret-key"
    },
    "user": {
      "name": "user",
      "key": "app-mgr-user-secret-key"
    }
  },
  "Labels": {
    "os_version": "centos7.6",
    "arch": "x86_64"
  },
  "RestApiPermissions": {
    "login": true,
    "auth": true,
    "view-app": true,
    "view-app-output": true,
    "view-all-app": true,
    "view-host-resource": true,
    "app-reg": true,
    "app-reg-shell": true,
    "app-control": true,
    "app-delete": true,
    "run-app-async": true,
    "run-app-sync": true,
    "run-app-async-output": true,
    "file-download": true,
    "file-upload": true,
    "label-view": true,
    "label-update": true
  }
}
