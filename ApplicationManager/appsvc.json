{
  "Description": "myhost",
  "ScheduleIntervalSeconds": 2,
  "RestListenPort": 6060,
  "RestListenAddress": "0.0.0.0",
  "RestEnabled": true,
  "LogLevel": "DEBUG",
  "SSLEnabled": true,
  "SSLCertificateFile": "server.crt",
  "SSLCertificateKeyFile": "server.key",
  "HttpThreadPoolSize": 6,
  "JWTEnabled": true,
  "Applications": [
    {
      "command_line": "/bin/sleep 60",
      "name": "ping",
      "user": "root",
      "working_dir": "/tmp"
    }
  ],
  "jwt": {
    "admin": {
      "name": "admin",
      "key": "app-mgr-admin-secret-key"
    },
    "user": {
      "name": "user",
      "key": "app-mgr-user-secret-key"
    }
  },
  "Lables": {
    "os_version": "centos7.6",
    "arch": "x86_64"
  }
}
