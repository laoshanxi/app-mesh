{
  "Description": "myhost",
  "ScheduleIntervalSeconds": 2,
  "LogLevel": "DEBUG",
  "REST": {
    "RestEnabled": true,
    "HttpThreadPoolSize": 6,
    "RestListenPort": 6060,
    "RestListenAddress": "0.0.0.0",
    "PrometheusExporterListenPort": 0,
    "SSL": {
      "SSLEnabled": true,
      "SSLCertificateFile": "/opt/appmanager/ssl/server.pem",
      "SSLCertificateKeyFile": "/opt/appmanager/ssl/server-key.pem"
    }
  },
  "Security": {
    "JWTEnabled": true,
    "JWTRedirectUrl": null,
    "JWTRedirectUrl_example": "https://192.168.2.1:6060",
    "EncryptKey": false,
    "Users": {
      "admin": {
        "key": "Admin123",
        "roles": [ "manage", "view", "usermgr" ],
        "locked": false
      },
      "user": {
        "key": "password",
        "roles": [ "view" ],
        "locked": false
      },
      "test": {
        "key": "test",
        "roles": [],
        "locked": true
      }
    },
    "Roles": {
      "manage": [
        "app-reg",
        "app-control",
        "app-delete",
        "run-app-async",
        "run-app-sync",
        "file-download",
        "file-upload",
        "label-view",
        "label-set",
        "label-delete",
        "config-set",
        "change-passwd",
        "lock-user",
        "unlock-user"
      ],
      "view": [
        "view-app",
        "view-app-output",
        "view-all-app",
        "view-host-resource",
        "run-app-async-output",
        "label-view",
        "config-view",
        "get-users",
        "role_view"
      ],
      "usermgr": [
        "add-user",
        "delete-user",
        "role_update"
      ]
    }
  },
  "Applications": [
    {
      "command": "ping www.baidu.com -w 300",
      "name": "ping",
      "user": "root",
      "cache_lines": 30
    }
  ],
  "Consul": {
    "is_master": true,
    "is_node": true,
    "url": null,
    "url_example": "http://localhost:8500",
    "datacenter": "dc1",
    "session_TTL": 30,
    "report_interval": 15,
    "schedule_interval": 10
  },
  "Labels": {
    "os_version": "centos7.6",
    "arch": "x86_64"
  }
}
