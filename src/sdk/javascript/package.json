{
  "name": "appmesh",
  "version": "1.0.9",
  "description": "JavaScript SDK for App Mesh (https://github.com/laoshanxi/app-mesh)",
  "main": "./src/appmesh.js",
  "module": "./dist/appmesh.esm.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "lint": "prettier --write .",
    "test": "node test/appmesh_test.js",
    "build": "webpack --config webpack.config.cjs --mode production",
    "prepare": "npm run build",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "node": "./src/appmesh.js",
      "import": "./dist/appmesh.esm.js",
      "default": "./dist/appmesh.esm.js"
    },
    "./server": {
      "node": "./src/server.js",
      "import": null,
      "default": null
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laoshanxi/app-mesh.git"
  },
  "homepage": "https://github.com/laoshanxi/app-mesh/tree/main/src/sdk/javascript",
  "keywords": [
    "app-mesh",
    "sdk",
    "api",
    "server",
    "tcp",
    "client"
  ],
  "author": "laoshanxi",
  "license": "MIT",
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "base-64": ">=1.0.0"
  },
  "optionalDependencies": {
    "msgpack-lite": "^0.1.26",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/runtime": "^7.23.0",
    "babel-loader": "^9.2.1",
    "buffer": "^6.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "base-64": ">=1.0.0"
  },
  "directories": {
    "test": "test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "maintained node versions",
    "not op_mini all",
    "ie 11"
  ]
}